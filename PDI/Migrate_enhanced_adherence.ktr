    <sql>
-- 29. Enhanced Adherence Screening
DROP TABLE IF EXISTS migration_st.st_enhanced_adherence;
CREATE TABLE migration_st.st_enhanced_adherence (
  Person_Id                        INT(11),
  Patient_clinic_number            VARCHAR(100),
  Encounter_Date                   DATE,
  Encounter_ID                     VARCHAR(50),
  Session_number                   INT(50),
  First_session_date               DATE,
  Pill_count                       TEXT,
  Arv_adherence                    TEXT,
  MMAS4AdherenceRating             TEXT,
  MMAS4Score                       TEXT,
  MMAS8Score                       TEXT,
  MMAS8AdherenceRating             TEXT,
  Has_vl_results                   TEXT,
  Vl_results_suppressed            TEXT,
  Vl_results_feeling               TEXT,
  Cause_of_high_vl                 TEXT,
  Way_forward                      TEXT,
  Patient_hiv_knowledge            TEXT,
  Patient_drugs_uptake             TEXT,
  Patient_drugs_reminder_tools     TEXT,
  Patient_drugs_uptake_during_travels TEXT,
  Patient_drugs_side_effects_response TEXT,
  Patient_drugs_uptake_most_difficult_times TEXT,
  Patient_drugs_daily_uptake_feeling TEXT,
  Patient_ambitions                  TEXT,
  Patient_has_people_to_talk         TEXT,
  Patient_enlisting_social_support   TEXT,
  Patient_income_sources            TEXT,
  Patient_challenges_reaching_clinicScreening TEXT,
  Patient_challenges_reaching_clinic TEXT,
  Patient_worried_of_accidental_disclosureScreening TEXT,
  Patient_worried_of_accidental_disclosure TEXT,
  Patient_treated_differentlyScreening TEXT,
  Patient_treated_differently        TEXT,
  Stigma_hinders_adherenceScreening  TEXT,
  Stigma_hinders_adherence           TEXT,
  Patient_tried_faith_healingScreening TEXT,
  Patient_tried_faith_healing       TEXT,
  Patient_adherence_improved         TEXT,
  Patient_doses_missed               TEXT,
  Review_and_barriers_to_adherence   TEXT,
  Other_referrals                    TEXT,
  Appointments_honoured              TEXT,
  Referral_experience                TEXT,
  Home_visit_benefit                 TEXT,
  Adherence_plan                     TEXT,
  Next_appointment_date              TEXT,   -- Should be a date
  CreatedBy                          INT(11),
  CreateDate                         DATE,
  Voided                             INT(11)
);</sql>
