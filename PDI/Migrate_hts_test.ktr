    <sql>-- 4. HTS TEST Encounter
DROP TABLE IF EXISTS migration_st.st_hts_test;
CREATE TABLE migration_st.st_hts_test
(
  Person_Id                        INT(11),
  Patient_clinic_number            VARCHAR(100),
  Patient_Id                       INT(11),
  Encounter_Date                   DATE,
  Encounter_ID                     VARCHAR(50),
  Pop_Type                         VARCHAR(255),
  Key_Pop_Type                     LONGTEXT,
  Priority_Pop_Type                TEXT,
  Patient_disabled                 TEXT,
  Disability                       TEXT,
  Ever_Tested                      VARCHAR(255),
  Self_Tested                      VARCHAR(255),
  MonthSinceSelfTest               VARCHAR(255),
  MonthsSinceLastTest              VARCHAR(255),
  HTS_Strategy                     VARCHAR(255),
  HTS_Entry_Point                  VARCHAR(255),
  Consented                        VARCHAR(255),
  Tested_As                        VARCHAR(255),
  TestType                         VARCHAR(255),
  Test_1_Kit_Name                  VARCHAR(255),
  Test_1_Lot_Number                VARCHAR(255),
  Test_1_Expiry_Date               DATE,
  Test_1_Final_Result              VARCHAR(255),
  Test_2_Kit_Name                  VARCHAR(255),
  Test_2_Lot_Number                VARCHAR(255),
  Test_2_Expiry_Date               DATE,
  Test_2_Final_Result              VARCHAR(255),
  Final_Result                     VARCHAR(255),
  Result_given                     VARCHAR(255),
  Couple_Discordant                VARCHAR(255),
  Tb_Screening_Results             VARCHAR(255),
  Remarks                          TEXT,
  CreatedBy                        INT(11),
  CreateDate                       DATE,
  Voided                           INT(11)
);</sql>
