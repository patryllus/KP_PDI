    <sql>-- 32A. Alcohol and drug abuse screening_cage
DROP TABLE IF EXISTS migration_st.st_cage_alcohol_screening;
CREATE TABLE migration_st.st_cage_alcohol_screening (
  Person_Id     						      INT(11),
  Patient_clinic_number                       VARCHAR(100),
  Encounter_Date					      	  DATE,
  Encounter_ID 						          VARCHAR(50),
  DrinkAlcohol 					        	  TEXT,
  Smoke 							          TEXT,
  UseDrugs						          	  TEXT,
  TriedStopSmoking 				    		  TEXT,
  FeltCutDownDrinkingorDrugUse 			      TEXT,
  AnnoyedByCriticizingDrinkingorDrugUse       TEXT,
  FeltGuiltyDrinkingorDrugUse 			      TEXT,
  UseToSteadyNervesGetRidHangover 		      TEXT,
  AlcoholRiskLevel						      TEXT,
  AlcoholScore 						          TEXT,
  Notes 								      TEXT

);
</sql>
