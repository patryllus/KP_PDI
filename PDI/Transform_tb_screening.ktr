    <sql>-- . TB Screening transformed table
DROP TABLE IF EXISTS migration_tr.tr_tb_screening;
CREATE TABLE migration_tr.tr_tb_screening
    select
      scr.Person_Id,
      scr.Patient_clinic_number,
      scr.Encounter_Date,
      scr.Encounter_ID,
      (case scr.Cough
       when "Yes" then 159799
       when "No" then 1066  else "" end)as Cough,
      (case scr.Fever
       when "Yes" then 1494
       when "No" then 1066  else "" end)as Fever,
      (case scr.Weight_loss
       when "Yes" then 832
       when "No" then 1066  else "" end)as Weight_loss,
      (case scr.Night_sweats
       when "Yes" then 133027
       when "No" then 1066  else "" end)as Night_sweats,
      (case scr.Tests_Ordered
       when "Yes" then 1065
       when "No" then 1066  else "" end)as Tests_Ordered,
      (case scr.Sputum_Smear
       when "Positive" then 703
       when "Ordered" then 307
       when "Negative" then 664  else "" end)as Sputum_Smear,
      (case scr.X_ray 
       when "Ordered" then 12
       when "Suggestive" then 152526
       when "Normal" then 1115
       when "Not Done" then 1118
       when "NotDone" then 1118 else "" end)as X_ray ,
      (case scr.Gene_xpert
       when "Ordered" then 162202
       when "Negative" then 664
       when "Positive" then 162204
       when "Not Done" then 1118
       when "NotDone" then 1118  else "" end)as Gene_xpert,     
      (case scr.Contact_tb_case
       when "Yes" then 1065
       when "No" then 1066  else "" end)as Contact_tb_case,           
      scr.Referral,
      scr.Clinical_diagnosis,
      (case scr.Invitation_contacts
       when "Yes" then 1065
       when "No" then 1066  else "" end)as Invitation_contacts,
      (case scr.Evaluated_for_IPT
       when "Yes" then 1065
       when "No" then 1066  else "" end)as Evaluated_for_IPT,
      (case scr.TB_results
       when "No TB Signs" then 1660
       when "No Signs" then 1660
       when "Presumed TB" then 142177
       when "TB Confirmed" then 1661
       when "TB Rx" then  1662
       when "INH" then 1679 
       when "Not Done" then  160737
       when "TB Screening Not Done" then  160737 else "" end)as TB_results,
       scr.created_at as Create_date,
       scr.created_by as Created_by 
    FROM migration_st.st_tb_screening scr
         LEFT JOIN migration_tr.tr_demographics dm ON dm.Person_Id = scr.Person_Id 
         WHERE (scr.Encounter_Date != "" OR scr.Encounter_Date IS NOT NULL) 
         AND (scr.TB_results != "Not Done") 
         GROUP BY scr.Person_Id, scr.Encounter_Date;</sql>
