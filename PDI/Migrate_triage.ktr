    <sql>
-- Add Field SP02 for table dtl_PatientVitals  
-- Issue is fouls in most versions below v2.2.0


IF COL_LENGTH('dtl_PatientVitals', 'SP02') IS NULL
BEGIN
    ALTER TABLE dtl_PatientVitals
    ADD [SP02] decimal(7,2)
END

</sql>
